<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>no.nav.bidrag.dokument</groupId>
  <artifactId>bidrag-dokument-dto-multimodule</artifactId>
  <version>0.3.2-SNAPSHOT</version>

  <modules>
    <module>bidrag-dokument-dto</module>
    <module>bidrag-dokument-dto-test</module>
  </modules>

  <description>multimodule build of bidrag-dokument</description>
  <url>https://github.com/navikt/bidrag-dokument-dto</url>
  <packaging>pom</packaging>

  <properties>

    <!-- language -->
    <java.version>12</java.version>
    <kotlin.version>1.3.41</kotlin.version>

    <!-- dependencies -->
    <junit-jupiter.version>5.5.1</junit-jupiter.version>

    <!-- build -->
    <maven-compiler.version>3.8.1</maven-compiler.version>
    <maven-surefire.version>2.22.0</maven-surefire.version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>

  </properties>

  <dependencies>
    <dependency>
      <groupId>org.junit.jupiter</groupId>
      <artifactId>junit-jupiter-api</artifactId>
      <version>${junit-jupiter.version}</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.junit.jupiter</groupId>
      <artifactId>junit-jupiter-engine</artifactId>
      <version>${junit-jupiter.version}</version>
      <scope>test</scope>
    </dependency>
    <dependency>
      <groupId>org.junit.jupiter</groupId>
      <artifactId>junit-jupiter-params</artifactId>
      <version>${junit-jupiter.version}</version>
      <scope>test</scope>
    </dependency>
  </dependencies>

  <scm>
    <url>https://github.com/navikt/bidrag-dokument-dto</url>
    <developerConnection>scm:git:https://github.com/navikt/bidrag-dokument-dto</developerConnection>
  </scm>

  <repositories>
    <repository>
      <id>local-releases</id>
      <name>Local stuff</name>
      <url>https://repo.adeo.no/repository/maven-releases</url>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
    </repository>
  </repositories>
  <distributionManagement>
    <repository>
      <id>m2internal</id>
      <name>NAV Maven2 Internal Repository</name>
      <url>http://maven.adeo.no/nexus/content/repositories/m2internal</url>
    </repository>
    <snapshotRepository>
      <id>m2snapshot</id>
      <name>NAV Maven2 Snapshot Repository</name>
      <url>http://maven.adeo.no/nexus/content/repositories/m2snapshot</url>
      <uniqueVersion>false</uniqueVersion>
    </snapshotRepository>
  </distributionManagement>

</project>
